        <section class="my-5" style="background-color:#0b0b0b;">
            <div class="container " id="loading-container">
                <div class="row">
                    <h1 class="text-white">Your Orders</h1>
                    <% for(let i=orders.length-1 ;i>=0; i--) { %>
                        <div class="col-lg-9">
                            <div class="card mb-4 ">
                                <div class="card-body d-flex justify-content-between">
                                    <div>
                                        <a href="/userorderDetailPage?id=<%= orders[i]._id %>"><img
                                                src="<%= orders[i].orderItems.image[0] %>" alt="hjcc"
                                                style="width: 150px;"></a>
                                    </div>
                                    <div>
                                        <p>orderid:<%= orders[i]._id %>
                                        </p>
                                        <p>productName:<%= orders[i].orderItems.pName %>
                                        </p>
                                        <p>Quantity:<%= orders[i].orderItems.quantity %>
                                        </p>
                                        <p>Price:<%=orders[i].orderItems.quantity*orders[i].orderItems.price %>
                                        </p>
                                        <p>order Date :<%=orders[i].orderDate.split('T')[0]%>
                                        </p>
                                    </div>
                                    <div class="mt-3">
                                        <h5>Status:<%=orders[i].orderItems.orderStatus %>
                                        </h5>
                                    </div>
                                    <div class="mt-3">
                                        <% if (orders[i].orderItems.orderStatus==='delivered' ) { %>
                                            <button onclick="downloadInvoice('<%=orders[i]._id %>')"><i
                                                    class="bi bi-receipt "></i></button>
                                            <button class="btn btn-lg" type="submit"
                                                style="text-decoration: none; font-weight: 500; color: #f4f4f4;background-color: red;"
                                                data-bs-toggle="modal"
                                                data-bs-target="#exampleModal1<%= i %>">RETURN</button>


                                            <div class="modal fade" id="exampleModal1<%= i %>" tabindex="-1"
                                                aria-labelledby="exampleModalLabel1<%= i %>" aria-hidden="true">
                                                <div class="modal-dialog">
                                                    <div class="modal-content">
                                                        <div class="modal-header">
                                                            <h5 class="modal-title" id="exampleModalLabel<%= i %>">
                                                                Return order</h5>
                                                            <button type="button" class="btn-close"
                                                                data-bs-dismiss="modal" aria-label="Close"></button>
                                                        </div>
                                                        <div class="modal-body">
                                                            <p>Are you sure you want to return this order?</p>
                                                            <form
                                                                action="/api/updateOrderStatusReturn?orderId=<%= orders[i]._id %>&productId=<%= orders[i].orderItems.productId %>"
                                                                method="post">

                                                                <div class="form-group">
                                                                    <label for="reason">Reason:</label>
                                                                    <textarea class="form-control" type="string"
                                                                        id="reason" name="reason" rows="3"
                                                                        placeholder="Enter your reason"
                                                                        required></textarea>
                                                                </div>
                                                        </div>
                                                        <div class="modal-footer">
                                                            <button type="button" class="btn btn-secondary"
                                                                data-bs-dismiss="modal">Close</button>

                                                            <button type="submit" class="btn btn-primary">ok</button>
                                                            </form>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>


                                            <% } else if (orders[i].orderItems.orderStatus==='ordered' ||
                                                orders[i].orderItems.orderStatus==='shipped' ||
                                                orders[i].orderItems.orderStatus==='pending' ||
                                                orders[i].orderItems.orderStatus==='pending' ) { %>

                                                <button class="btn btn-lg bg-primary" data-bs-toggle="modal"
                                                    data-bs-target="#exampleModal<%= i %>"
                                                    style="text-decoration: none; font-weight: 500; color: #f4f4f4;">cancel</button>


                                                <div class="modal fade" id="exampleModal<%= i %>" tabindex="-1"
                                                    aria-labelledby="exampleModalLabel<%= i %>" aria-hidden="true">
                                                    <div class="modal-dialog">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                                <h5 class="modal-title" id="exampleModalLabel<%= i %>">
                                                                    Modal title</h5>
                                                                <button type="button" class="btn-close"
                                                                    data-bs-dismiss="modal" aria-label="Close"></button>
                                                            </div>
                                                            <div class="modal-body">
                                                                <p>Are you sure you want to cancel this order?</p>
                                                                <form
                                                                    action="/api/updateOrderStatusCancel?orderId=<%= orders[i]._id %>&productId=<%= orders[i].orderItems.productId %>"
                                                                    method="post">
                                                                    <div class="form-group">
                                                                        <label for="reason">Reason:</label>
                                                                        <textarea class="form-control" type="string"
                                                                            id="reason" name="reason" rows="3"
                                                                            placeholder="Enter your reason"
                                                                            required></textarea>
                                                                    </div>
                                                            </div>


                                                            <div class="modal-footer">
                                                                <button type="button" class="btn btn-secondary"
                                                                    data-bs-dismiss="modal">Close</button>
                                                                <button type="submit"
                                                                    class="btn btn-primary">ok</button>
                                                                </form>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <% } %>
                                    </div>
                                </div>
                            </div>


                        </div>
                        <% } %>

